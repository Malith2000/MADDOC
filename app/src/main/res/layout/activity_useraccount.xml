    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#F1C107"
        app:cardBackgroundColor="#FFC107"
        app:cardElevation="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton23"
                android:layout_width="93dp"
                android:layout_height="86dp"
                android:layout_gravity="left|center_vertical"
                android:backgroundTint="#FFC107"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/logo" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right|center_vertical"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton22"
                    android:layout_width="50dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center_vertical"
                    android:backgroundTint="#FFC107"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/account_avatar_profile_human_man_user_30448" />

                <ImageButton
                    android:id="@+id/imageButton21"
                    android:layout_width="48dp"
                    android:layout_height="47dp"
                    android:layout_gravity="center_vertical"
                    android:backgroundTint="#FFC107"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/_26083" />

                <ImageButton
                    android:id="@+id/imageButton24"
                    android:layout_width="54dp"
                    android:layout_height="46dp"
                    android:layout_gravity="center_vertical"
                    android:backgroundTint="#FFC107"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/png_transparent_hamburger_button_computer_icons_drop_down_list_fast_food_menu_rectangle_share_icon_pancake_thumbnail" />
            </LinearLayout>
        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <TextView
